\n\t{ src: \"/db/videos/video.mp4\", preview: \"/db/videos/preview.png\", rejected: true },\r\n\t{ src: \"/db/videos/video.mp4\", preview: \"/db/videos/preview.png\" },\r\n]\r\n\r\nexport default function VideoPage () {\r\n\r\n\tconst { data } = useSWR('/api/videos', GET, { refreshInterval: 5000 })\r\n\r\n\tconsole.log(data)\r\n\r\n\tconst onClickItem = (item) => {\r\n\t\topenModalVideo(item)\r\n\t}\r\n\r\n\treturn (\r\n\t\t<>\r\n\t\t\t<header>\r\n\t\t\t\t<h1>Видео-поздравления</h1>\r\n\t\t\t</header>\r\n\t\t\t<div className=\"block-list\">\r\n\t\t\t\t{data && data.map((item, index) => (\r\n\t\t\t\t\t<button className={cn(\"item\", item.rejected && \"rejected\")} key={index} onClick={() => onClickItem(item)}>\r\n\t\t\t\t\t\t<img src={item.preview} alt=\"Превью-видео\"/>\r\n\t\t\t\t\t\t<div className=\"icons\">\r\n\t\t\t\t\t\t\t{ typeof item.seconds === 'number' && <div className=\"progress\">{item.seconds}с.</div> }\r\n\t\t\t\t\t\t\t{ item.transcoded && <IoIosVideocam/> }\r\n\t\t\t\t\t\t\t{ item.accepted && <IoIosCheckmarkCircle/> }\r\n\t\t\t\t\t\t\t{ item.rejected && <IoIosCloseCircle color=\"red\"/> }\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</button>\r\n\t\t\t\t))}\r\n\t\t\t</div>\r\n\t\t</>\r\n\t)\r\n}",["360","361"],"C:\\Users\\fox\\Desktop\\Works\\nipi\\cms\\src\\components\\modal-window\\modal-confirm.jsx",[],"C:\\Users\\fox\\Desktop\\Works\\nipi\\cms\\src\\components\\modal-window\\modal-video.jsx",["362"],"import React, { useState } from 'react'\r\nimport cn from 'classnames'\r\nimport { REST } from 'libs/fetch'\r\nimport { mutate } from 'swr'\r\n\r\nimport { modal, ModalBase, closeModal } from './index'\r\n\r\nexport function ModalVideo({item}){\r\n\r\n\tconst [ state, setState ] = useState(item)\r\n\r\n\tconst accept = async () => {\r\n\t\r\n\t\tconst resp = await REST('/api/videos/'+item._id, { accepted: true, rejected: false }, 'PUT')\r\n\t\tsetState({...item, accepted: true, rejected: false })\r\n\t\tif(!resp.error) mutate('/api/videos')\r\n\t}\r\n\r\n\tcons